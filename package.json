{
  "name": "next-web3-boilerplate",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc --noEmit -p tsconfig.json",
    "lint-eslint": "eslint --ignore-path .eslintignore --ext .js,.jsx,.ts,.tsx",
    "lint-prettier": "prettier --ignore-path .prettierignore",
    "contracts:compile": "typechain --target ethers-v5 --out-dir contracts/types \"./contracts/abis/**/*.json\"",
    "postinstall": "yarn contracts:compile",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.@(js|jsx|ts|tsx)": [
      "npm run lint-eslint -- --fix",
      "npm run lint-prettier -- --write"
    ]
  },
  "dependencies": {
    "@emotion/cache": "^11.7.1",
    "@emotion/react": "^11.8.1",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.8.1",
    "@ethersproject/bignumber": "^5.5.0",
    "@ethersproject/contracts": "^5.5.0",
    "@ethersproject/providers": "^5.5.2",
    "@ethersproject/units": "^5.5.0",
    "@ethersproject/wallet": "^5.5.0",
    "@metamask/detect-provider": "^1.2.0",
    "@metamask/onboarding": "^1.0.1",
    "@mui/icons-material": "^5.5.1",
    "@mui/material": "^5.5.0",
    "@web3-react/core": "^6.1.9",
    "@web3-react/injected-connector": "^6.0.7",
    "@web3-react/walletconnect-connector": "^6.2.13",
    "axios": "^0.26.1",
    "clsx": "^1.1.1",
    "crypto-js": "^4.1.1",
    "graphql": "^16.3.0",
    "graphql-request": "^4.2.0",
    "next": "12.1.0",
    "nprogress": "^0.2.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-multi-carousel": "^2.8.0",
    "react-query": "^3.35.0",
    "recoil": "^0.7.3-alpha.2",
    "swr": "^1.2.0"
  },
  "devDependencies": {
    "@typechain/ethers-v5": "^9.0.0",
    "@types/crypto-js": "^4.1.1",
    "@types/node": "^17.0.21",
    "@types/react": "^17.0.38",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "eslint": "8.8.0",
    "eslint-config-next": "12.0.9",
    "eslint-config-prettier": "^8.4.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-react": "^7.29.2",
    "eslint-plugin-react-hooks": "^4.3.0",
    "ethers": "^5.5.3",
    "husky": "^7.0.0",
    "lint-staged": "^12.3.4",
    "prettier": "^2.5.1",
    "typechain": "^7.0.0",
    "typescript": "^4.5.5"
  }
}
